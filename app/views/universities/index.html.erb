<p id="notice"><%= notice %></p>

<h1>Університети</h1>

<table>
  <thead>
    <tr>
      <th>Назва</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @universities.each do |university| %>
      <tr>
        <td><%= university.full_name %></td>
        <td><%= link_to 'Показати', university_path(university.url) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
