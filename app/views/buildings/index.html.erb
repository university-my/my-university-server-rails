<p id="notice"><%= notice %></p>

<h1>Buildings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buildings.each do |building| %>
      <tr>
        <td><%= link_to building.name, university_building_url(@university.url, building.friendly_id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
