<p id="notice"><%= notice %></p>

<h1>Auditoriums</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditoriums.each do |auditorium| %>
      <tr>
        <td><%= auditorium.id %></td>
        <td><%= auditorium.name %></td>
        <td><%= link_to 'Show', auditorium %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
